month = feb,
pages = {bbt002--},
pmid = {23413432},
title = {{Gene set analysis methods: statistical models and methodological differences.}},
url = {http://bib.oxfordjournals.org/content/early/2013/02/09/bib.bbt002.full},
year = {2013}
}


	journal = "Physiological genomics",
	month = mar,
	number = "5",
	pages = "305--17",
	pmid = "22234994",
	title = "{Gene prioritization for livestock diseases by data integration.}",
	url = "http://physiolgenomics.physiology.org/cgi/content/abstract/44/5/305",
	volume = "44",
	year = "2012"
}




	title = "{Prediction of total genetic value using genome-wide dense marker maps}",
	url = "http://www.genetics.org/cgi/content/abstract/157/4/1819; http://www.genetics.org/content/157/4/1819.short",
	volume = "157",
	year = "2001"
}

